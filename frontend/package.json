  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "lighthouse": "^12.0.0",
    "postcss": "^8.5.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.14",
    "terser": "^5.43.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "payment-gateway",
    "fintech",
    "dashboard",
    "react",
    "typescript",
    "vite"
  ],
  "author": "LightSpeedPay Team",
  "license": "MIT"
}
