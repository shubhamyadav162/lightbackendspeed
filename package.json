{
  "name": "lightspeedpay-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "start": "node server-express.js",
    "start:next": "node .next/standalone/server.js",
    "start:express": "node server-express.js",
    "start:railway": "npm run build && node server-express.js",
    "start:production": "node server-express.js",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "workers": "ts-node src/workers/scheduler.ts",
    "workers:railway": "node -r ts-node/register src/workers/index.ts",
    "worker:simple-test": "ts-node src/workers/simple-test-worker.ts",
    "worker:queue-metrics": "ts-node src/workers/queue-metrics-recorder/index.ts",
    "worker:health-ping": "ts-node src/workers/worker-health-ping/index.ts",
    "worker:health-monitor": "ts-node src/workers/worker-health-monitor/index.ts",
    "worker:low-balance": "ts-node src/workers/low-balance-notifier/index.ts",
    "worker:webhook-retry": "ts-node src/workers/webhook-retry/index.ts",
    "worker:system-health": "ts-node src/workers/system-health-report/index.ts",
    "worker:sla-monitor": "ts-node src/workers/sla-monitor/index.ts",
    "worker:commission-payout": "ts-node src/workers/commission-payout-processor/index.ts",
    "worker:archive-transactions": "ts-node src/workers/archive-transactions/index.ts",
    "worker:queue-metrics-refresh": "ts-node src/workers/refresh-queue-metrics-view/index.ts",
    "migrate": "npx supabase db diff --schema=public --use-migra -f",
    "migrate:apply": "npx supabase db push",
    "migrate:new": "npx supabase migration new",
    "test:deno": "deno test supabase/functions/failed-transaction-alerts --allow-env",
    "test:e2e": "playwright test || echo \"Playwright tests skipped: environment unsupported\"",
    "enforce:codecov": "node scripts/enforce-codecov.js",
    "railway:deploy": "npm run build && node server-express.js",
    "railway:workers": "npm run workers:railway",
    "check:gateways": "node scripts/check-gateways.js",
    "create:test-merchant": "node scripts/create-test-merchant.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "bullmq": "^5.6.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "glob": "^11.0.3",
    "ioredis": "^5.3.2",
    "jose": "^5.2.3",
    "lru-cache": "^10.2.0",
    "next": "14.1.0",
    "pg": "^8.11.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.44.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20",
    "@types/pg": "^8.10.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "supabase": "^1.64.8",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": "20.x"
  }
}
